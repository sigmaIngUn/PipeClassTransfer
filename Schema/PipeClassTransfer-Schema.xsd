					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="S" type="xs:decimal" use="required">
					<xs:annotation>
						<xs:documentation>
							Wall-Thickness
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Type" use="required">
					<xs:annotation>
						<xs:documentation>
							Type of Blank
						</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
                        <xs:restriction base="xs:integer">
